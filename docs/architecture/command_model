- update_config       

{
  target: INI_TARGET, // list that
  value: INI_MODEL // dynamic
}

<pre>{<br>target:&nbsp;&lt;INIT_TARGET&gt;<br>value:&nbsp;&lt;INI_MODEL&gt;<br>}</pre>

VALUE = Union[STRING, ENUM, FLOAT, INT] // text field, select, number field, number field for int
INI_MODEL: Dict[str, Union[INI_MODEL, UNION[List[VALUE], VALUE]]]


- replace_matrix        

{
  target: INPUT_SERIES_MATRIX // list that
  value: MATRIX // check size
}
<pre>{<br>target:&nbsp;&lt;INPUT_SERIES_MATRIX&gt;<br>value:&nbsp;&lt;MATRIX&gt;<br>}</pre>

